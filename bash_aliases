alias screenfetch="screenfetch -n"
alias nano="nano -Scalgmwz"
alias fnano="sudo nano -Scalgmwz"
alias ls="ls -AN"
alias long="ls -ALNR --si"
alias rm="rm -Irvf --preserve-root"
alias ..="cd .."
alias ~="cd ~"
alias clean="clear"
alias killall="killall -iv"
alias fuck="killall -iv"
alias cp="cp -rvi"
alias ps="ps -erm"
alias top="htop"
alias mv="mv -iv"
